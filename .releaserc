{
    "branch": "master",
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/exec",
            {
                "prepareCmd": "echo `##vso[task.setvariable variable=nextVersion]${nextRelease.version}",
                "publishCmd": "nuget push ${process.env.BUILD_ARTIFACTSTAGINGDIRECTORY}\\SandboxLibrarySample.${nextRelease.version}.nupkg -Source https://api.nuget.org/v3/index.json -ApiKey ${process.env.NUGET_TOKEN}",
            }
        ],
        [
            "@semantic-release/github", 
            {
                "assets": 
                [
                    {"path": "${process.env.BUILD_ARTIFACTSTAGINGDIRECTORY}\\*.nupkg", "label": "NuGet package"},
                ]
            }
        ],
    ]
}
